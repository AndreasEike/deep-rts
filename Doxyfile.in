OUTPUT_DIRECTORY       = @CMAKE_CURRENT_SOURCE_DIR@/docs/api/
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/include/ @CMAKE_CURRENT_SOURCE_DIR@/src
INPUT += @CMAKE_CURRENT_SOURCE_DIR@/README.md
USE_MDFILE_AS_MAINPAGE = README.md
DOXYFILE_ENCODING      = UTF-8
RECURSIVE              = YES
PROJECT_NAME           = "Deep RTS"
PROJECT_LOGO           = @CMAKE_CURRENT_SOURCE_DIR@/docs/logo.png
EXTRACT_ALL = YES
FILE_PATTERNS          = *.c \
                         *.cc \
                         *.cxx \
                         *.cpp \
                         *.c++ \
                         *.h \
                         *.hh \
                         *.hxx \
                         *.hpp \
                         *.h++
